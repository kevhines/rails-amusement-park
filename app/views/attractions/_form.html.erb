<%=form_for(@attraction) do |f|%>
    <%=f.label("Name:") %>
    <%=f.text_field(:name) %>
    <%=f.label("Height Requirements:") %>
    <%=f.text_field(:min_height) %>
    <%=f.label("Nausea Inducing?") %>
    <%=f.text_field(:nausea_rating) %>
    <%=f.label("How fun?") %>
    <%=f.text_field(:happiness_rating) %>
    <%=f.label("How many tickets:") %>
    <%=f.text_field(:tickets) %>
    <%=f.submit %>
  <%end%>