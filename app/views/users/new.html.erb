<h1>Sign Up</h1>


<%= form_for @user do |f| %>
  <%=f.label "Name"%>
  <%=f.text_field :name%>
  <%=f.label "Height"%>
  <%=f.text_field :height%>
  <%=f.label "happiness"%>
  <%=f.text_field :happiness%>
  <%=f.label "nausea"%>
  <%=f.text_field :nausea%>
  <%=f.label "tickets"%>
  <%=f.text_field :tickets%>
  <%=f.label "Admin?"%>  
  <%= f.check_box :admin %> 
  <%=f.label "Password"%>
  <%=f.password_field :password %>
  <%=f.submit "Create User"%>
<%end%>